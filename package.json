{
    "name": "@elevatormedia/french-press",
    "version": "0.52.5",
    "scripts": {
        "dev": "nodemon --exec babel-node src/client.js",
        "test": "mocha --recursive \"test/*.spec.js\"",
        "release": "bump --commit --tag --push",
        "github-publish": "yarn release && npm publish"
    },
    "description": "Utility package to extract and insert social media usernames from urls",
    "main": "src/frenchPress.js",
    "author": "Jesse Meza <jmeza081@gmail.com>",
    "license": "UNLICENSED",
    "devDependencies": {
        "@babel/core": "^7.8.4",
        "@babel/node": "^7.8.4",
        "@babel/preset-env": "^7.8.4",
        "chai": "^4.2.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.9.0",
        "eslint-plugin-import": "^2.20.0",
        "eslint-plugin-react": "^7.18.0",
        "husky": "^1.2.0",
        "mocha": "^7.0.1",
        "nodemon": "^2.0.2",
        "nyc": "^15.0.0",
        "prettier": "^1.15.3",
        "pretty-quick": "^1.8.0"
    },
    "repository": {
        "url": "git://github.com/elevatormedia/french-press.git"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged",
            "pre-push": "yarn test"
        }
    },
    "dependencies": {
        "validator": "^12.2.0"
    }
